{
  "name": "tiarebalbi-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@atlaskit/css-reset": "^5.0.9",
    "@atlaskit/drawer": "^5.1.0",
    "@atlaskit/flag": "^12.3.4",
    "@atlaskit/form": "^6.3.2",
    "@atlaskit/icon": "^19.0.11",
    "@atlaskit/pagination": "^12.0.16",
    "@atlaskit/theme": "^9.3.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-flexbox-grid": "^2.1.2",
    "react-icons-kit": "^1.3.1",
    "react-loadable": "^5.5.0",
    "react-scripts": "3.3.0",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "package": "node scripts/exportArticles.js",
    "start": "yarn package && react-scripts start",
    "build": "yarn package && react-scripts build",
    "test": "yarn package && react-scripts test",
    "ci": "yarn test --watchAll=false",
    "postbuild": "react-snap"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn package && lint-staged"
    }
  },
  "devDependencies": {
    "chalk": "^3.0.0",
    "husky": "^3.1.0",
    "jimp": "^0.9.3",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react-snap": "^1.23.0",
    "request": "^2.88.0"
  }
}
